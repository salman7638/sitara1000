<odoo>
  <data>
    <record id="sale_order_com_form_view" model="ir.ui.view">
			<field name="name">sale.order.form.view.allotee</field>
			<field name="model">sale.order</field>
			<field name="inherit_id" ref="sale.view_order_form"/>
			<field name="arch" type="xml">
                <xpath expr="//field[@name='dealer_id']" position="after">
                    <field name="commission_date" />
                </xpath>   
           </field>
       </record> 
  </data>
</odoo>