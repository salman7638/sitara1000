<odoo>
    <template id="plot_pdf_report">
        <t t-call="web.html_container">
                <t t-foreach="docs" t-as="docs">
                    <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id">
                        <div class="page">
                            <div style="font-size:10px" class="col-12">
                            

                                <table style="border: 1px solid black;">
                                    <thead>
                                        <th style="border: 1px solid black;" width="50">Phase</th>
                                        <th style="border: 1px solid black;" width="100">Category</th>
                                        <th style="border: 1px solid black;" width="150">No Of Plots</th>
                                        <th style="border: 1px solid black;" width="150">Total Marla's</th>
                                        <th style="border: 1px solid black;" width="200">Available Plots</th>
                                        <th style="border: 1px solid black;" width="300">Available Plot Marla's</th>
                                        <th style="border: 1px solid black;" width="250">Un-Confirm Plots</th>
                                        <th style="border: 1px solid black;" width="350">Un-Confirm Plots Marla's</th>
                                        <th style="border: 1px solid black;" width="350">Confirm Plotss</th>
                                        <th style="border: 1px solid black;" width="350">Confirm Plots Marla's</th>
                                        <th style="border: 1px solid black;" width="350">Booked Plots</th>
                                        <th style="border: 1px solid black;" width="350">Booked Plots Marla's</th>
                                        <th style="border: 1px solid black;" width="350">Sold Plots</th>
                                        <th style="border: 1px solid black;" width="350">Sold Plots Marla's</th>
                                    </thead>
                                    <tbody>
                                        <tr t-foreach="my_list" t-as="plo">
                                            <td style="border: 1px solid black;">
                                                    <span t-esc="plo['plot_phase']"/>
                                                
                                            </td>
                                            <td style="border: 1px solid black;">
                                                
                                                <span t-esc="plo['plot_category']"/>

                                            </td>
                                             <td style="border: 1px solid black;">
                                                <span t-esc="plo['total_number_of_plots']"/>
                                            </td>
                                             <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['total_number_of_marlas']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['available_total_number_of_plots']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(int(plo['available_total_number_of_marlas']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['unconfirm_total_number_of_plots']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['unconfirm_total_number_of_marlas']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['reserve_total_number_of_plots']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['reserve_total_number_of_marlas']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['booked_total_number_of_plots']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['booked_total_number_of_marlas']))"/>
                                            </td>
                                            
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['sold_total_number_of_plots']))"/>
                                            </td>
                                            <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['sold_total_number_of_marlas']))"/>
                                            </td>
                                        </tr>
                                    <tr t-foreach="my_list" t-as="plo">
                                        <td style="border: 1px solid black;">
                                                <span t-esc="'{0:.2f}'.format(float(plo['grand_total_number_of_plots']))"/>
                                            </td>
                            
                                        </tr>
                                      
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                                    
                               
                         
                                 
                </t>
            </t>
    </template>

    <report id="plot_status_pdf_report_data"
            model="plot.status.wizard"
            string="Plot Status Pdf Report"
            report_type="qweb-pdf"
            name="de_property_report.plot_pdf_report"
            file="de_property_report.plot_pdf_report"
    />
</odoo>





